  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>USAidReport</DataSourceName>
        <CommandText>SELECT
  CountriesAndContinents.Continent
  ,FiscalYears.n
  ,CountriesAndContinents.Country
  ,FinancialAid.CountryName
  ,FinancialAid.ProgramName AS [FinancialAid ProgramName]
  ,FinancialAid.FiscalYear
  ,FinancialAid.Amount
  ,ProgramType.ProgramName AS [ProgramType ProgramName]
FROM
  FinancialAid
  INNER JOIN FiscalYears
    ON FinancialAid.FiscalYear = FiscalYears.n
  INNER JOIN ProgramType
    ON FinancialAid.ProgramName = ProgramType.ProgramName
  INNER JOIN CountriesAndContinents
    ON CountriesAndContinents.Country = FinancialAid.CountryName</CommandText>
